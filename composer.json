{
	"name": "mikolajzieba/yii2-rest-client",
	"type": "yii2-extension",
	"description": "REST client (AR-like model) for Yii Framework 2.0 (via yii2-http-client, extends ApexWire/yii2-restclient)",
	"keywords": [
		"yii2",
		"rest",
		"client",
		"model",
		"active record"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "ApexWire",
			"email": "apexwire@gmail.com"
		},
		{
			"name": "simialbi",
			"email": "simi.albi@gmail.com"
		}
	],
	"support": {
		"source": "https://github.com/simialbi/yii2-rest-client",
		"issues": "https://github.com/simialbi/yii2-rest-client/issues"
	},
	"minimum-stability": "stable",
	"require": {
		"php": ">=5.5.0",
		"yiisoft/yii2": ">=2.0.5",
		"yiisoft/yii2-httpclient": ">=2.0.0"
	},
	"autoload": {
		"psr-4": {
			"mikolajzieba\\yii2\\rest\\": ""
		}
	}
}
